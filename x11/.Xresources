! Colors Definition

#include "/home/jamie/.colorsX"

! Color Overrides
!URxvt*background: [90]#111111
URxvt*background: #111111
URxvt*foreground: #e8dfd6
XTerm*background: #111111
XTerm*foreground: #e8dfd6

! URxvt Configuration
URxvt*.url-launcher: /usr/bin/chromium
! Must be 0 if you want image previews to work
URxvt*depth: 0
URxvt*transparent: false
URxvt.scrollstyle: rxvt
URxvt.scrollBar: off
*cursorBlink: True
URxvt.letterSpace: 0
URxvt*imLocale: en_US.UTF-8 
*.cursorUnderline: False
*.cursorColor: #af865a
URxvt*termName: xterm
URxvt*internalBorder: 4
URxvt*perl-ext-common: default,matcher,clipboard 

! Fonts
 
*font:	-*-tamsyn-medium-r-*-*-17-*-*-*-*-*-iso8859-*
*boldFont:	-*-tamsyn-medium-r-*-*-17-*-*-*-*-*-iso8859-*

!URxvt*font: -aaron-bitocra13-*-*-normal-*-13-*-*-*-*-*-*-*
!URxvt*boldFont: -aaron-bitocra13-*-*-normal-*-13-*-*-*-*-*-*-*

Xterm*font: -*-gohufont-medium-r-*-*-14-*-*-*-*-*-iso10646-1
Xterm*boldFont: -*-gohufont-medium-r-*-*-14-*-*-*-*-*-iso10646-1
