Host *.home
  ProxyCommand ssh -q {jumphost} 'nc -w 900 `echo %h | sed s/\\.home$//` %p'

Host github.com
	User git
	IdentityFile {devkey}
	HostName github.com

Host bitbucket.org
	User git
	IdentityFile {devkey}
	HostName bitbucket.org

Host *
  ControlMaster auto
  ControlPath ~/.ssh/master-%r@%h:%p

